journal = {IEEE Transactions on Vehicular Technology},
keywords = {4IWD,Act:BTD,Act:MD,Base:IEEE,Control:Fuzzy,Control:FuzzyPID,Control:PID,Electric vehicle,Type:J,electronic stability control (ESC),in-wheel motor,independent drive,torque distribution},
